          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import java.nio.file.*

// 取 token
String token = vars.get(&quot;token&quot;)

if (token &amp;&amp; token != &quot;NOT_FOUND&quot;) {

    String dir = &quot;C:/Users/ajie/Desktop/&quot;
    String filePath = dir + &quot;tokens.txt&quot;

    try {
        // 自动创建目录
        Files.createDirectories(Paths.get(dir))

        String content = token + System.lineSeparator()

        Files.write(
            Paths.get(filePath),
            content.getBytes(&quot;UTF-8&quot;),
            StandardOpenOption.CREATE,
            StandardOpenOption.APPEND
        )

        log.info(&quot;写入 token 成功: &quot; + token)

    } catch (Exception e) {
        log.error(&quot;写文件失败: &quot;, e)
    }

} else {
    log.warn(&quot;token 未找到，当前线程：&quot; + ctx.threadNum)
}
</stringProp>
